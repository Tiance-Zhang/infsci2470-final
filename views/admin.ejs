   <div class="row justify-content-center">
     
     <% if (cart) { %>
    <p>There are <%= cart.length %>students in the database.</p>
   <table class="table table-bordered" style="max-width:80%;">
      <% cart.forEach(function(cart){ %>
      <tr><td>Task_email: <%= cart.email %></td></tr>
     <table class="table table-bordered" style="max-width:60%;">
     
      <% cart.product_list.forEach(function(product){ %>
<table class="table table-bordered" style="max-width:0%;">
      <tr><td>Task_id: <%= product.Task_id %></td></tr>
      <tr><td>NAME: <%= product.TaskName %></td></tr>
  
  <%= if(product.status){%>
      <tr><td>status: <%= product.status %></td></tr>
  <% }; %>
  
   </table>
        <br/><br/><br/><br/> 
  <% }); %>
     </table>
      <tr><td></td></tr>
  <% }); %>
    </table>
    <% } else { %>
    <p>No students in the database.</p>
    <% } %>
    
