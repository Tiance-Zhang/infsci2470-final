<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Shoes</title>
  <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css"
      integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS"
      crossorigin="anonymous"
    />
</head>
<body>
  <div class="container" style="padding-top: 20px">
  <nav class="nav">
    <li class="nav-item">
      <a class="nav-link active" href="/">Home</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" href="/product2">Check All Shoes</a>
    </li>
    <li class="nav-item">
      <a class="nav-link active" href="/addNew">Add New Shoes</a>
    </li>
    <li class="nav-item">
      <a class="nav-link active" href="/vendors">Check Brand</a>
    </li>
  </nav>
  
   <div>
    
    <ul class="list-group">
      
      <% vendors.vendors.forEach(function(item) { %>
      <div class="card">
        <li class="list-group-item">ID: <%= item.id %></li>
        <li class="list-group-item">NAME: <%= item.name %></li>
        <li class="list-group-item">EMAIL: <%= item.email %></li>
        <li class="list-group-item">TEL: <%= item.tel %></li>
        <li class="list-group-item">Sign: <%= item.sign %></li>
        <li class="list-group-item"><a href="/deleteVendor/<%= item.id %>/vendor">Delete</a></li>
      </div>
      <br>
      <% }) %>
     
    </ul>
    
  </div>
    
  </div>
</body>
</html>
